<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:srcCompat="@drawable/bg_img"
        android:scaleType="centerCrop"
        />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="35mm"
        android:layout_height="55mm"
        android:layout_marginTop="4mm"
        app:srcCompat="@drawable/carica" />

    <ImageView
        android:id="@+id/imageView13"
        android:layout_width="75mm"
        android:layout_height="55mm"
        android:layout_marginLeft="32mm"
        android:layout_marginTop="4mm"
        android:layout_marginRight="5mm"
        app:srcCompat="@drawable/instruc" />

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="20mm"
        android:layout_height="10mm"
        android:layout_marginTop="1mm"
        app:srcCompat="@drawable/back1" />


</RelativeLayout>